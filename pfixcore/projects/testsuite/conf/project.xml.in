<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="testsuite" xmlns:cus="http://www.schlund.de/pustefix/customize">
  
  <active>true</active>
  <comment>Test suite for pfixcore</comment>
  <depend>testsuite/conf/depend.xml</depend>
  
  <virtualhost><cus:fqdn/></virtualhost>
  <servername>testsuite.<cus:fqdn/></servername>
  <serveralias>testsuite.<cus:machine/></serveralias>
  
  <ports>
    <port number="80"/>
  </ports>
  
  <defpath>/xml/main</defpath>
  <documentroot><cus:docroot/>testsuite/htdocs</documentroot>
  
  <servlet name="main" useineditor="true">
    <active>true</active>
    <class>de.schlund.pfixxml.ContextXMLServlet</class>
    <propfile>testsuite/conf/main.conf.xml</propfile>
  </servlet>
  
  <servlet name="test1" useineditor="true">
    <active>true</active>
    <class>de.schlund.pfixxml.ContextXMLServlet</class>
    <propfile>testsuite/conf/test1.conf.xml</propfile>
  </servlet>
  
</project>
